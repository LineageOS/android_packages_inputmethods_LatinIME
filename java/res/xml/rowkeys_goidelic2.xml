<merge
    xmlns:latin="http://schemas.android.com/apk/res/com.android.inputmethod.latin"
>
    <!-- a,s,d,f,g -->
    <Key
        latin:keySpec="a"
        latin:keyHintLabel="\@"
        latin:additionalMoreKeys="\\@"
        latin:moreKeys="!text/morekeys_a" />
    <Key
        latin:keySpec="s"
        latin:keyHintLabel="#"
        latin:additionalMoreKeys="#"
        latin:moreKeys="!text/morekeys_s" />
    <!-- Changed dollar to pound -->
    <Key
        latin:keySpec="d"
        latin:keyHintLabel="&#x00A3;"
        latin:additionalMoreKeys="&#x00A3;"
        latin:moreKeys="!text/morekeys_d" />
    <!-- Added morekeys_f -->
    <Key
        latin:keySpec="f"
        latin:keyHintLabel="%"
        latin:additionalMoreKeys="%"
        latin:moreKeys="!text/morekeys_f" />
    <!-- Changed ampersand to Tironian Sign Et -->
    <Key
        latin:keySpec="g"
        latin:keyHintLabel="&#x204A;"
        latin:additionalMoreKeys="&#x204A;"
        latin:moreKeys="!text/morekeys_g" />
    <!-- h,j,k,l -->
    <include
        latin:keyboardLayout="@xml/rowkeys_qwerty2_right4" />
</merge>
